{
  "assistant": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4o"
    },
    "version": "2"
  },
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "project_panel": {
    "dock": "right"
  },
  "preferred_line_length": 100,
  "soft_wrap": "preferred_line_length",
  "buffer_font_family": "Victor Mono",
  "tab_size": 2,
  "file_types": {
    "SQL": ["ddl"]
  },
  "show_whitespaces": "all"
}
